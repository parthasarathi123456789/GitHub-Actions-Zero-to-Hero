import boto3
from botocore.exceptions import NoCredentialsError

# Initialize S3 client
s3 = boto3.client('s3')

# Your bucket name
bucket_name = 'your-bucket-name'

def upload_file(local_file, s3_file):
    try:
        s3.upload_file(local_file, bucket_name, s3_file)
        print(f"‚úÖ Uploaded {local_file} to {bucket_name}/{s3_file}")
    except FileNotFoundError:
        print("‚ùå File not found")
    except NoCredentialsError:
        print("‚ùå AWS credentials not found")

def download_file(s3_file, local_file):
    try:
        s3.download_file(bucket_name, s3_file, local_file)
        print(f"‚úÖ Downloaded {s3_file} to {local_file}")
    except Exception as e:
        print(f"‚ùå Error: {e}")

def list_files():
    try:
        response = s3.list_objects_v2(Bucket=bucket_name)
        print("üìÇ Files in bucket:")
        for obj in response.get('Contents', []):
            print(f" - {obj['Key']}")
    except Exception as e:
        print(f"‚ùå Error: {e}")

# Test functions
if __name__ == "__main__":
    list_files()
    upload_file("test.txt", "uploaded/test.txt")
    download_file("uploaded/test.txt", "downloaded_test.txt")
